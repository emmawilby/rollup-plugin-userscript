{
  "name": "@emmawilby/rollup-plugin-userscript",
  "version": "0.2.1",
  "description": "Fork of violentmonkey's Rollup plugin for userscript",
  "author": "Gerald <gera2ld@live.com>,Emma <me@emma.gay>",
  "license": "MIT",
  "scripts": {
    "ci": "run-s lint",
    "lint": "eslint src",
    "dev": "rollup -wc",
    "clean": "del-cli dist",
    "build:js": "rollup -c",
    "build": "run-s ci clean build:js",
    "prepare": "husky install",
    "prepublishOnly": "run-s build"
  },
  "type": "module",
  "module": "dist/index.js",
  "exports": {
    "import": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "rollup",
    "userscript"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.10",
    "@rollup/pluginutils": "^5.0.3",
    "estree-walker": "^3.0.3",
    "is-reference": "^3.0.1"
  },
  "devDependencies": {
    "@gera2ld/plaid": "~2.5.9",
    "@gera2ld/plaid-rollup": "~2.5.9",
    "del-cli": "^5.0.0",
    "husky": "^8.0.3"
  },
  "repository": "git@github.com:emmawilby/rollup-plugin-userscript.git",
  "engines": {
    "node": ">=16"
  }
}
